&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="71"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="71"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 59.8pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="80"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 154.65pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="206"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 3cm; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="113"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: #ece9d8; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 10cm; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 18pt; background-color: transparent" width="378"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 20pt; color: #333333"&gt;　&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;tr style="height: 33pt"&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: windowtext 1pt solid; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="71"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;序号&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="71"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;姓名&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 59.8pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="80"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;学号&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 154.65pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="206"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;学科专业&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 3cm; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="113"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;导师&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 10cm; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="378"&gt;
&lt;div align="center"&gt;&lt;b&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;论文题目&lt;/span&gt;&lt;/b&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;tr style="height: 33pt"&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: windowtext 1pt solid; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="71"&gt;
&lt;div align="center"&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;1&lt;/span&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 53.2pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="71"&gt;
&lt;div&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;瞿海霞&lt;/span&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 59.8pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="80"&gt;
&lt;div align="center"&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;1099188&lt;/span&gt;&lt;/div&gt;
&lt;/td&gt;
&lt;td nowrap="nowrap" style="border-right: windowtext 1pt solid; padding-right: 5.4pt; border-top: #ece9d8; padding-left: 5.4pt; padding-bottom: 0cm; border-left: #ece9d8; width: 154.65pt; padding-top: 0cm; border-bottom: windowtext 1pt solid; height: 33pt; background-color: transparent" width="206"&gt;
&lt;div align="center"&gt;&lt;span style="font-size: 14pt; color: #333333"&gt;机械制造及其自动化&lt;/span&gt;&lt;/div&gt;
&lt;/td&gt;
